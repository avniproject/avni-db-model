// Framework
 Table "audit" {
 	"id" integer  [default: `nextval('audit_id_seq'::regclass)`]
 	"uuid" varchar(255)
 	"created_by_id" bigint  [ref: > users.id]
 	"last_modified_by_id" bigint [ref: > users.id]
 	"created_date_time" timestamp
 	"last_modified_date_time" timestamp
 	Note: 'A common table for audit information of all other tables - except a few where the table themselves keep the audit information. It is not an audit log of all changes over the life time of a table row - rather it is only creation and last modification information.'
 }


// Account
Table "account" {
 	"id" integer  [default: `nextval('account_id_seq'::regclass)`] 
 	"name" varchar(255)  
 } 
 
 Table "account_admin" {
 	"id" integer  [default: `nextval('account_admin_id_seq'::regclass)`] 
 	"name" varchar(255)  
 	"account_id" integer [ref: > account.id]
 	"admin_id" integer   
 } 

TableGroup Account {
    account
    account_admin
}


// Organisation
Table "organisation_group" {
 	"id" integer  [default: `nextval('organisation_group_id_seq'::regclass)`]
 	"name" varchar(255)
 	"db_user" varchar(255)
 	"account_id" integer [ref: > account.id]
 }

 Table "organisation_group_organisation" {
 	"id" integer  [default: `nextval('organisation_group_organisation_id_seq'::regclass)`]
 	"name" varchar(255)
 	"organisation_group_id" integer [ref: > organisation_group.id]
 	"organisation_id" integer [ref: > organisation.id]
 	Note: ''
 }

Table "organisation" {
  	"id" integer  [default: `nextval('organisation_id_seq'::regclass)`]
  	"name" varchar(255)
  	"db_user" varchar(255) [note: 'The database user for this organisation. using row-level security this database user is provided only access to organisational data.']
  	"uuid" varchar(255)
  	"parent_organisation_id" integer [ref: > organisation.id]
  	"is_voided" boolean  [default: false]
  	"media_directory" text [note: 'Name of the director in S3 where this organisation's files are stored']
  	"username_suffix" text [note: 'The suffix which is appended to each user in the organisation, to keep them unique across organisations']
  	"account_id" integer  [default: 1, ref: > account.id]
  	Note: 'Organisation which is using the Avni (or a tenant in a multi-tenant system). If you are logged in using organisation database user you will see only your organisation or group if you are master organisation in the group.'
  }

TableGroup Organisation {
    organisation
    organisation_group
    organisation_group_organisation
}



// Work area / Location

 Table "address_level" {
 	"id" integer  [default: `nextval('address_level_id_seq'::regclass)`] 
