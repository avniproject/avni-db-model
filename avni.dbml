  Table "location_location_mapping" {
  	"id" integer  [default: `nextval('location_location_mapping_id_seq'::regclass)`]
  	"location_id" bigint [ref: > address_level.id]
  	"parent_location_id" bigint [ref: > address_level.id]
  	"version" integer
  	"audit_id" bigint [ref: > audit.id]
